<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="316736148" author="kuba">
        <createTable tableName="parking">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="adress" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="capacity" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="parking_type" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="parking_spot_width" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="parking_spot_length" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="taken_places" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="places_for_electric_cars" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="taken_electric_places" type="int">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>